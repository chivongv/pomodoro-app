{
  "name": "pomodoro-app",
  "version": "0.2.0",
  "description": "A pomodoro app built with electron.js and react.js",
  "main": "main.js",
  "keywords": [
    "reactjs",
    "electron.js"
  ],
  "author": {
    "name": "Chi Vong",
    "email": "chivongv@gmail.com"
  },
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron-react": "concurrently \"BROWSER=none npm start\" \"wait-on http://localhost:3000 && electron .\""
